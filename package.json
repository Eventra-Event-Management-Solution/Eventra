{
  "name": "Eventra",
  "version": "1.0.0",
  "main": "public/electron.js",
  "homepage": "./",
  "author": "Eventra Team",
  "description": "Event Management Solution for Event Management Companies",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
