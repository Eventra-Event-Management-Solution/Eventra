    "date-fns": "^4.1.0",
    "firebase": "^12.0.0",
    "formik": "^2.4.6",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1",
    "react-scripts": "^5.0.1",
